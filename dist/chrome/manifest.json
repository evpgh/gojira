{
  "manifest_version": 3,
  "name": "GoJira",
  "version": "1.1",
  "omnibox": {
    "keyword": "gj"
  },
  "permissions": [
    "storage"
  ],
  "options_page": "options_ui/page.html",
  "background": {
    "service_worker": "background/service_worker.js"
  },
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "web_accessible_resources/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}